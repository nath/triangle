let
    var s: string
in
    begin
    s := "app";
    if ("hi" ++ "yo") = "hiyo" then 
        put('y')
    else
        put('n');

    if (s <<< "apple") = 0 then
        put('y')
    else
        put('n');
    
    if ((s ++ "le") <<< "apple") = 1 then
        put('y')
    else
        put('n');

    if ("apple" <<< s) = 2 then
        put('y')
    else
        put('n');

    put(s[0]);
    put(s[1]);
    put(s[2]);

    if s[0] = 'a' then
        put('y')
    else
        put('n')
    end
